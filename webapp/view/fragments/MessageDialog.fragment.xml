<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog afterClose="onDialogAfterClose" draggable="true" title="Hata Günlüğü" icon="sap-icon://status-error">
		<List items="{message>/}">
			<StandardListItem title="{message>title}" highlight="Error"/>
		</List>
		<beginButton>
			<Button text="Kapat" press="onCloseDialog" icon="sap-icon://decline"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>